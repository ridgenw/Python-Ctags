import shutil
import os


def delete(path):
    try:
        path2 = (path + "/pyrepo")
        try:
            shutil.rmtree(path2)
        except:
            print(os.getcwd())
        try:
            os.system('rm tags')
        except:
            print("Removing tags file could not be done.")
        try:
            shutil.rmtree(path + "/ctags")
        except:
            return
    except OSError as e:
        print(e.strerror)


delete("/Users/ridgewilliams/Developer/VSCode/Python")